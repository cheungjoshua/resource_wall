<header>
  <div class="header_logo">RESOURCE WALL</div>
  <nav>
    <a href="/">Home</a>
    <a href="/explore">Explore</a>
    <a href="/resources">Add Resource</a>
  </nav>
  <div class="logCheck">
    <% if (typeof user !== "undefined") { %>
    <p>Logged In As User: <%= user %></p>
    <button class="logout">Logout</button>
    <% } else { %>
    <button class="loginButton" style="display: block">Login</button>
    <div class="userLogin" style="display: none">
      <button id="1" class="userButton" value="1">user1</button>
      <button id="2" class="userButton" value="2">user2</button>
      <button id="3" class="userButton" value="3">user3</button>
    </div>
    <% } %>
  </div>
</header>
