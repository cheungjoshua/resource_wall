<!DOCTYPE html>
<html lang="en">

<head>
  <title>Add New Resource</title>

  <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" type="text/css" />
  <link rel="stylesheet" href="/vendor/border-box.css" type="text/css" />
  <link rel="stylesheet" href="/styles/main.css" type="text/css" />
  <link rel="stylesheet" href="/styles/layout.css" type="text/css" />

  <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>
  <script type="text/javascript" src="/scripts/app.js"></script>
  <script type="text/javascript" src="/scripts/newResource.js"></script>
  <script type="text/javascript" src="/scripts/loginbutton.js"></script>

</head>

<body>
  <%- include('partials/_header') %>
    <main>
      <h1>Add New Resource</h1>

      <div class="create_new_resource_form_wrapper">
        <form actions="/" method="POST" id="create_new_resource_form">
          <label for="create_new_resource_form-url">Link URL</label>
          <input type="url" name="url" placeholder="link url" id="create_new_resource_form-url"> </br>
          <label for="create_new_resource_form-title">Title</label>
          <input type="text" name="title" placeholder="title" id="create_new_resource_form-title"></br>
          <label for="create_new_resource_form-description">Description</label>
          <input type="text" name="description" placeholder="description" id="create_new_resource_form-description"></br>
          <!-- <label for="creat_new_resource_form-tag">Tag</label>
          <input type="'text" name="tag" placeholder="tag" id="creat_new_resource_form-tag"> </br> -->
          <input type="submit" name="submit" class="create_new_resource_form-submit" value="submit"></input>
        </form>

        <div class="added_alert">
          <h6 class="alert">Added!</h6>
        </div>
      </div>
    </main>
</body>



</html>
